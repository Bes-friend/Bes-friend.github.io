{
  "name": "Bes-friend.github.io",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://besfriend.org/",
  "dependencies": {
    "cra-template": "1.1.2",
    "gh-pages": "^3.2.3",
    "react": "^16.14.0",
    "react-dom": "^16.5.0",
    "react-intl": "^5.24.0",
    "react-markdown": "^7.1.1",
    "react-render-html": "^0.6.0",
    "react-scripts": "4.0.3",
    "survey-react": "^1.8.79"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -b main -d build",
    "i18n_extract": "formatjs extract 'src/**/*.js' --out-file extracted.json",
    "i18n_compile": "formatjs compile extracted.json --out-file src/lang/en.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "^4.5.0",
    "babel-plugin-formatjs": "^10.3.12"
  }
}
